{
  "entrypoint": "index.html",
  "fragments": [],
  "sources": [
    "dist/**",
    "index.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "HTML",
      "browserCapabilities": [
        "es2015",
        "push",
        "serviceworker",
        "modules"
      ],
      "html": {
        "minify": false
      },
      "css": {
        "minify": false
      },
      "js": {
        "minify": false,
        "compile": false
      },
      "bundle": false,
      "addServiceWorker": false,
      "addPushManifest": false
    }
  ],
  "lint": {
    "rules": [
      "polymer-3"
    ]
  },
  "npm": true,
  "componentDir": "node_modules/",
  "moduleResolution": "node"
}